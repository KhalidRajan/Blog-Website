<%- include("partials/header"); %>


<h1>Home</h1>
<p><%=homePageContent%></p>

<% posts.forEach(function(post){ %>
  <h1><%= post.title%></h1>
  <% postContent=post.content.substring(0, 100)+"..." %>
  <p><%= postContent %> <a href="/posts/<%= post.title %>">Read More</a> </p>

<% })%>

